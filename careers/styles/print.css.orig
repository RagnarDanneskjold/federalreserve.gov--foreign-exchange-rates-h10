/*------------------Styles for FAQs Page-------------------------------------------------------------------------------------------*/
ol.faq {margin-right: 15px;}
ol.faq li{font-weight: bold; color: #10367E; padding-top: 15px; font-size: 1.1em; font-family:Arial, Helvetica, sans-serif;}
.answer {margin-left: 10px; margin-top: 7px; font-family: Arial, Helvetica, sans-serif; color: #000000; font-weight:normal; }
.answer2 {margin-left: 195px; margin-top: 10px; font-family: Arial, Helvetica, sans-serif; color: #000000; font-weight:normal; }
