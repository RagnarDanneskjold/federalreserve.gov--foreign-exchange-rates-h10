/*Telerik RadCalendar Default skin*/

table.RadCalendar_CRASkin
{
	background:#fff;
	font:12px "segoe ui",arial,sans-serif;
}

/*titlebar*/

.RadCalendar_CRASkin .rcTitlebar
{
	border-color:#868686 #868686 #c4c4c4;
	background:#eaeaea 0 -1000px repeat-x url('Calendar/sprite.gif');
	color:#000;
}

.RadCalendar_CRASkin .rcTitlebar table
{
	font:12px/23px "segoe ui",arial,sans-serif;
}

.RadCalendar_CRASkin .rcTitlebar td
{
	padding:0 0 1px;
}

.RadCalendar_CRASkin .rcTitlebar .rcPrev,
.RadCalendar_CRASkin .rcTitlebar .rcNext,
.RadCalendar_CRASkin .rcTitlebar .rcFastPrev,
.RadCalendar_CRASkin .rcTitlebar .rcFastNext
{
	background-image:url('Calendar/sprite.gif');
	color:#ccc;
}

.RadCalendar_CRASkin .rcTitlebar .rcFastPrev
{
	margin-left:6px;
	background-position:1px -197px;
}

.RadCalendar_CRASkin .rcTitlebar a.rcFastPrev:hover
{
	background-position:1px -247px;
}

.RadCalendar_CRASkin .rcTitlebar .rcPrev
{
	background-position:4px -297px;
}

.RadCalendar_CRASkin .rcTitlebar a.rcPrev:hover
{
	background-position:4px -347px;
}

.RadCalendar_CRASkin .rcTitlebar .rcNext
{
	background-position:3px -397px;
}

.RadCalendar_CRASkin .rcTitlebar a.rcNext:hover
{
	background-position:3px -447px;
}

.RadCalendar_CRASkin .rcTitlebar .rcFastNext
{
	margin-right:6px;
	background-position:1px -497px;
}

.RadCalendar_CRASkin .rcTitlebar a.rcFastNext:hover
{
	background-position:1px -547px;
}

.RadCalendar_CRASkin .rcMain
{
	border-color:#868686;
}

.RadCalendar_CRASkin .rcMainTable
{
	font:12px/17px "segoe ui",arial,sans-serif;
}

/*header, footer*/

.RadCalendar_CRASkin .rcHeader,
.RadCalendar_CRASkin .rcFooter
{
	border-color:#868686;
}

/*week numbers and days*/

.RadCalendar_CRASkin .rcWeek th
{
	border-bottom:1px solid #c5c5c5;
	padding:4px 6px 3px 0;
	color:#333;
}

.RadCalendar_CRASkin .rcWeek .rcViewSel,
.RadCalendar_CRASkin .rcRow th
{
	width:13px;
	padding:0 7px 0 9px;
	background:#eee;
}

.RadCalendar_CRASkin .rcRow th
{
	color:#777;
}

/*date cells*/

.RadCalendar_CRASkin .rcRow td
{
	border-color:#fff;
}

.RadCalendar_CRASkin .rcMain .rcRow a,
.RadCalendar_CRASkin .rcMain .rcRow span
{
	color:#333;
}

.RadCalendar_CRASkin .rcMain .rcWeekend a
{
	color:#666;
}

.RadCalendar_CRASkin .rcRow .rcToday
{
	border-color:#898989;
}

.RadCalendar_CRASkin .rcMain .rcOtherMonth a
{
	color:#898989;
}

.RadCalendar_CRASkin .rcMain .rcOutOfRange span
{
	color:#898989;
}

.RadCalendar_CRASkin .rcRow .rcSelected
{
	border-color:#8d8d8d #7d7d7d #6c6c6c;
	background:#828282 0 -1700px repeat-x url('Calendar/sprite.gif');
}

.RadCalendar_CRASkin .rcMain .rcRow .rcSelected a
{
	color:#fff;
}

.RadCalendar_CRASkin .rcRow .rcHover
{
	border-color:#c4c4c4 #b2b2b2 #9e9e9e;
	background:#c5c5c5 0 -1600px repeat-x url('Calendar/sprite.gif');
}

.RadCalendar_CRASkin .rcMain .rcRow .rcHover a
{
	color:#333;
}

/*multimonth view*/

.RadCalendarMultiView_CRASkin .rcTitlebar
{
	border-color:#9a9a9a #9c9c9c #9c9c9c;
	background:#c5c5c5 0 -1199px repeat-x url('Calendar/sprite.gif');
}

.RadCalendarMultiView_CRASkin .rcTitlebar table
{
	border:1px solid;
	border-color:#fdfdfd #f3f3f3 #e7e7e7;
	line-height:27px;
}

.RadCalendarMultiView_CRASkin .rcTitlebar a.rcFastPrev:hover
{
	background-position:1px -647px;
}

.RadCalendarMultiView_CRASkin .rcTitlebar a.rcPrev:hover
{
	background-position:4px -747px;
}

.RadCalendarMultiView_CRASkin .rcTitlebar a.rcNext:hover
{
	background-position:3px -847px;
}

.RadCalendarMultiView_CRASkin .rcTitlebar a.rcFastNext:hover
{
	background-position:1px -947px;
}

.RadCalendarMultiView_CRASkin .rcMain
{
	border-color:#9b9b9b;
}

.RadCalendarMultiView_CRASkin .rcCalendar
{
	border-color:#868686;
}

.RadCalendarMultiView_CRASkin .rcCalendar .rcMainTable
{
	height:176px;
}

.RadCalendarMultiView_CRASkin .rcMainTable .rcTitle
{
	border-color:#c4c4c4;
	padding:0 0 2px;
	background:#eaeaea 0 -1000px repeat-x url('Calendar/sprite.gif');
	line-height:22px;
}

/*month view*/

table.RadCalendarMonthView_CRASkin
{
	border-color:#979797;
	background:#fff;
	font:12px "segoe ui",arial,sans-serif;
	color:#333;
}

.RadCalendarMonthView_CRASkin #rcMView_Feb,
.RadCalendarMonthView_CRASkin #rcMView_Apr,
.RadCalendarMonthView_CRASkin #rcMView_Jun,
.RadCalendarMonthView_CRASkin #rcMView_Aug,
.RadCalendarMonthView_CRASkin #rcMView_Oct,
.RadCalendarMonthView_CRASkin #rcMView_Dec
{
	border-right:1px solid #e0e0e0;
}

.RadCalendarMonthView_CRASkin a
{
	color:#333;
}

.RadCalendarMonthView_CRASkin .rcSelected a
{
	border-color:#8d8d8d #7d7d7d #6c6c6c;
	background:#828282 0 -1700px repeat-x url('Calendar/sprite.gif');
	color:#fff;
}

.RadCalendarMonthView_CRASkin #rcMView_PrevY a,
.RadCalendarMonthView_CRASkin #rcMView_NextY a
{
	width:17px;
	height:14px;
	background-image:url('Calendar/sprite.gif');
	color:#ccc;
}

.RadCalendarMonthView_CRASkin #rcMView_PrevY a
{
	background-position:7px -196px;
}

.RadCalendarMonthView_CRASkin #rcMView_NextY a
{
	background-position:7px -496px;
}

.RadCalendarMonthView_CRASkin .rcButtons
{
	padding:6px 7px 5px;
}

.RadCalendarMonthView_CRASkin input
{
	border-color:#a7a7a7 #7b7b7b #7b7b7b #a7a7a7;
	background:#e9e9e9 0 -2300px repeat-x url('Calendar/sprite.gif');
	color:#000;
	font:12px "segoe ui",arial,sans-serif;
}

/*time view*/

table.RadCalendarTimeView_CRASkin
{
	border-color:#868686;
	background:#fff;
	font:12px "segoe ui",arial,sans-serif;
}

.RadCalendarTimeView_CRASkin th
{
	border-bottom:1px solid #c4c4c4;
	padding:0 0 1px;
	background:#eaeaea 0 -1000px repeat-x url('Calendar/sprite.gif');
	color:#000;
	line-height:23px;
}

table.RadCalendarTimeView_CRASkin td
{
	border-color:#c5c5c5;
}

.RadCalendarTimeView_CRASkin a
{
	color:#333;
}

.RadCalendarTimeView_CRASkin td.rcSelected a
{
	border-color:#8d8d8d #7d7d7d #6c6c6c;
	background:#828282 0 -1700px repeat-x url('Calendar/sprite.gif');
	color:#fff;
}

.RadCalendarTimeView_CRASkin td.rcHover a
{
	border-color:#c4c4c4 #b2b2b2 #9e9e9e;
	background:#c5c5c5 0 -1600px repeat-x url('Calendar/sprite.gif');
	color:#000;
}

.RadCalendarTimeView_CRASkin .rcFooter
{
	border-color:#c5c5c5;
}

/*pickers*/

.RadPicker_CRASkin .rcCalPopup,
.RadPicker_CRASkin .rcTimePopup
{
	background-image:url('Calendar/sprite.gif');
}

.RadPicker_CRASkin .rcCalPopup,
.RadPicker_CRASkin a.rcDisabled.rcCalPopup:hover
{
	background-position:0 0;
}

.RadPicker_CRASkin a.rcCalPopup:hover,
.RadPicker_CRASkin a.rcCalPopup:focus,
.RadPicker_CRASkin a.rcCalPopup:active
{
	background-position:0 -50px;
}

.RadPicker_CRASkin .rcTimePopup,
.RadPicker_CRASkin a.rcDisabled.rcTimePopup:hover
{
	background-position:0 -100px;
}

.RadPicker_CRASkin a.rcTimePopup:hover,
.RadPicker_CRASkin a.rcTimePopup:focus,
.RadPicker_CRASkin a.rcTimePopup:active
{
	background-position:0 -150px;
}

/*rtl*/

.RadCalendarRTL_CRASkin .rcTitlebar .rcFastPrev
{
	background-position:1px -497px;
}

.RadCalendarRTL_CRASkin .rcTitlebar a.rcFastPrev:hover
{
	background-position:1px -547px;
}

.RadCalendarRTL_CRASkin .rcTitlebar .rcPrev
{
	background-position:3px -397px;
}

.RadCalendarRTL_CRASkin .rcTitlebar a.rcPrev:hover
{
	background-position:3px -447px;
}

.RadCalendarRTL_CRASkin .rcTitlebar .rcNext
{
	background-position:4px -297px;
}

.RadCalendarRTL_CRASkin .rcTitlebar a.rcNext:hover
{
	background-position:4px -347px;
}

.RadCalendarRTL_CRASkin .rcTitlebar .rcFastNext
{
	background-position:1px -197px;
}

.RadCalendarRTL_CRASkin .rcTitlebar a.rcFastNext:hover
{
	background-position:1px -247px;
}

.RadCalendarMultiViewRTL_CRASkin .rcTitlebar a.rcFastPrev:hover
{
	background-position:1px -947px;
}

.RadCalendarMultiViewRTL_CRASkin .rcTitlebar a.rcPrev:hover
{
	background-position:3px -847px;
}

.RadCalendarMultiViewRTL_CRASkin .rcTitlebar a.rcNext:hover
{
	background-position:4px -747px;
}

.RadCalendarMultiViewRTL_CRASkin .rcTitlebar a.rcFastNext:hover
{
	background-position:1px -647px;
}