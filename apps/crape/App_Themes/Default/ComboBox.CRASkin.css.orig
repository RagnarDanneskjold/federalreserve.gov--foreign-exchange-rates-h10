/* RadComboBox Default skin */

/*global*/

.RadComboBox_CRASkin,
.RadComboBox_CRASkin .rcbInput,
.RadComboBoxDropDown_CRASkin
{
	font: 12px "Segoe UI", Arial, sans-serif;
	color: #333;
}

/* combobox */

.RadComboBox_CRASkin .rcbInputCellLeft,
.RadComboBox_CRASkin .rcbInputCellRight,
.RadComboBox_CRASkin .rcbArrowCellLeft,
.RadComboBox_CRASkin .rcbArrowCellRight
{
	background-image: url('ComboBox/rcbSprite.png');
}

* html .RadComboBox_CRASkin .rcbInputCellLeft,
* html .RadComboBox_CRASkin .rcbInputCellRight,
* html .RadComboBox_CRASkin .rcbArrowCellLeft,
* html .RadComboBox_CRASkin .rcbArrowCellRight
{
	background-image: url('ComboBox/rcbSpriteIE6.png');
}

.RadComboBox_CRASkin td.rcbInputCellLeft { background-position: 0 0; }
.RadComboBox_CRASkin td.rcbInputCellRight { background-position: 100% 0; }

.RadComboBox_CRASkin .rcbHovered .rcbInputCellLeft { background-position: 0 0; }
.RadComboBox_CRASkin .rcbHovered .rcbInputCellRight { background-position: 100% 0; }

.RadComboBox_CRASkin .rcbFocused .rcbInputCellLeft { background-position: 0 0; }
.RadComboBox_CRASkin .rcbFocused .rcbInputCellRight { background-position: 100% 0; }

.RadComboBox_CRASkin .rcbHovered .rcbReadOnly td.rcbInputCellLeft { background-position: 0 -22px; }
.RadComboBox_CRASkin .rcbHovered .rcbReadOnly td.rcbInputCellRight { background-position: 100% -22px; }

.RadComboBox_CRASkin .rcbFocused .rcbReadOnly td.rcbInputCellLeft { background-position: 0 -44px; }
.RadComboBox_CRASkin .rcbFocused .rcbReadOnly td.rcbInputCellRight { background-position: 100% -44px; }

.RadComboBox_CRASkin .rcbInputCell .rcbEmptyMessage
{
	color: #666;
}

.RadComboBox_CRASkin .rcbHovered .rcbInputCell .rcbInput
{
	color: #000;
}

.RadComboBox_CRASkin td.rcbArrowCellLeft { background-position: -18px -88px; }
.RadComboBox_CRASkin td.rcbArrowCellRight { background-position: 0 -88px; }

.RadComboBox_CRASkin .rcbHovered .rcbArrowCellLeft { background-position: -54px -88px; }
.RadComboBox_CRASkin .rcbHovered .rcbArrowCellRight { background-position: -36px -88px; }

.RadComboBox_CRASkin .rcbFocused .rcbArrowCellLeft { background-position: -90px -88px; }
.RadComboBox_CRASkin .rcbFocused .rcbArrowCellRight { background-position: -72px -88px; }

.RadComboBox_CRASkin td.rcbArrowCellHidden,
.RadComboBox_CRASkin .rcbHovered td.rcbArrowCellHidden,
.RadComboBox_CRASkin .rcbFocused td.rcbArrowCellHidden { background-position: -15px -88px; }

.RadComboBox_CRASkin_rtl td.rcbArrowCellHidden,
.RadComboBox_CRASkin_rtl .rcbHovered td.rcbArrowCellHidden,
.RadComboBox_CRASkin_rtl .rcbFocused td.rcbArrowCellHidden { background-position: -18px -88px; }

/* Read-only styles */

.RadComboBox_CRASkin .rcbReadOnly td.rcbArrowCellLeft { background-position: -18px -88px; }
.RadComboBox_CRASkin .rcbReadOnly td.rcbArrowCellRight { background-position: -2px -88px; }

.RadComboBox_CRASkin .rcbHovered .rcbReadOnly .rcbArrowCellLeft { background-position: -54px -88px; }
.RadComboBox_CRASkin .rcbHovered .rcbReadOnly .rcbArrowCellRight { background-position: -38px -88px; }

.RadComboBox_CRASkin .rcbFocused .rcbReadOnly .rcbArrowCellLeft { background-position: -90px -88px; }
.RadComboBox_CRASkin .rcbFocused .rcbReadOnly .rcbArrowCellRight { background-position: -74px -88px; }

.RadComboBox_CRASkin .rcbReadOnly td.rcbArrowCellHidden { background-position: -15px -88px; }
.RadComboBox_CRASkin .rcbHovered .rcbReadOnly td.rcbArrowCellHidden { background-position: -51px -88px; }
.RadComboBox_CRASkin .rcbFocused .rcbReadOnly td.rcbArrowCellHidden { background-position: -87px -88px; }

.RadComboBox_CRASkin_rtl .rcbReadOnly td.rcbArrowCellHidden { background-position: -18px -88px; }
.RadComboBox_CRASkin_rtl .rcbHovered .rcbReadOnly td.rcbArrowCellHidden { background-position: -54px -88px; }
.RadComboBox_CRASkin_rtl .rcbFocused .rcbReadOnly td.rcbArrowCellHidden { background-position: -90px -88px; }

/* dropdown */

.RadComboBoxDropDown_CRASkin .rcbHeader,
.RadComboBoxDropDown_CRASkin .rcbFooter,
.RadComboBoxDropDown_CRASkin .rcbMoreResults,
.RadComboBoxDropDown_CRASkin .rcbMoreResults a
{
	background-image: url('ComboBox/rcbSprite.png');
}

* html .RadComboBoxDropDown_CRASkin .rcbHeader,
* html .RadComboBoxDropDown_CRASkin .rcbFooter,
* html .RadComboBoxDropDown_CRASkin .rcbMoreResults,
* html .RadComboBoxDropDown_CRASkin .rcbMoreResults a
{
	background-image: url('ComboBox/rcbSpriteIE6.png');
}

.RadComboBoxDropDown_CRASkin
{
	background: #fff;
	border-color: #c6c7d2;
	border-top-color: #83868d;
}

.RadComboBoxDropDown_CRASkin .rcbHeader,
.RadComboBoxDropDown_CRASkin .rcbFooter
{
	background-color: #f1f2f4;
	background-position: 0 -110px;
}

.RadComboBoxDropDown_CRASkin .rcbHeader
{
	border-bottom-color: #d5d5d5;
}

.RadComboBoxDropDown_CRASkin .rcbFooter
{
	border-top-color: #d5d5d5;
}

.RadComboBoxDropDown_CRASkin .rcbItem em
{
	background: #e5e5e5;
}

.RadComboBoxDropDown_CRASkin .rcbHovered
{
	background: #dfdfdf;
	color: #000;
}

.RadComboBoxDropDown_CRASkin .rcbSeparator
{
    color: #fff;
    background: #8a8a8a;
}

.RadComboBox_CRASkin .rcbDisabled .rcbInputCell .rcbInput,
.RadComboBoxDropDown_CRASkin .rcbDisabled
{
	color: #999;
}

.RadComboBox_CRASkin .rcbDisabled td.rcbInputCellLeft { background-position: 0 -66px; }
.RadComboBox_CRASkin .rcbDisabled td.rcbInputCellRight { background-position: 100% -66px; }
.RadComboBox_CRASkin .rcbDisabled td.rcbArrowCellLeft { background-position: -126px -88px; }
.RadComboBox_CRASkin .rcbDisabled td.rcbArrowCellRight { background-position: -108px -88px; }
.RadComboBox_CRASkin .rcbDisabled .rcbReadOnly td.rcbArrowCellRight { background-position: -110px -88px; }
.RadComboBox_CRASkin table.rcbDisabled td.rcbArrowCellHidden,
.RadComboBox_CRASkin .rcbDisabled .rcbReadOnly td.rcbArrowCellHidden { background-position: -123px -88px; }
.RadComboBox_CRASkin_rtl table.rcbDisabled td.rcbArrowCellHidden,
.RadComboBox_CRASkin_rtl .rcbDisabled .rcbReadOnly td.rcbArrowCellHidden { background-position: -126px -88px; }

.RadComboBoxDropDown_CRASkin .rcbLoading
{
	background: #f0f0f0;
}

.RadComboBoxDropDown_CRASkin .rcbMoreResults
{
	border-top-color: #c6c7d2;
	background-position: 0 -132px;
	background-color: #e6e6e6;
	color: #8a8a8a;
}

.RadComboBoxDropDown_CRASkin .rcbMoreResults a
{
	background-position: -157px -94px;
}
